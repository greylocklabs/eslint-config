{
    "name": "@greylocklabs/eslint-config",
    "version": "1.12.0",
    "description": "Greylock Labs ESLint rules for all internal and public JavaScript projects",
    "author": {
        "name": "Ty-Lucas Kelley",
        "email": "ty@greylocklabs.com",
        "url": "https://greylocklabs.com"
    },
    "homepage": "https://github.com/greylocklabs/js/tree/master/eslint-config",
    "bugs": "https://github.com/greylocklabs/js/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/greylocklabs/js"
    },
    "license": "MIT",
    "keywords": [
        "eslint",
        "eslintconfig",
        "config",
        "javascript",
        "es2017",
        "greylocklabs",
        "styleguide",
        "lint",
        "flowtype"
    ],
    "scripts": {
        "lint": "npm run lint:js && npm run lint:md",
        "lint:js": "eslint --report-unused-disable-directives index.js rules/** test/index.js",
        "lint:md": "markdownlint README.md CHANGELOG.md",
        "pretest": "npm run lint",
        "test": "ava test/index.js",
        "posttest": "eslint-find-rules --unused",
        "prepublishOnly": "npm run test"
    },
    "main": "index.js",
    "engines": {
        "node": ">= 6"
    },
    "ava": {
        "require": "babel-register",
        "babel": "inherit",
        "verbose": true
    },
    "devDependencies": {
        "ava": "0.25.0",
        "babel-cli": "6.26.0",
        "babel-eslint": "8.2.2",
        "babel-preset-env": "1.6.1",
        "eslint": "4.18.1",
        "eslint-find-rules": "3.2.1",
        "eslint-plugin-ava": "4.5.1",
        "eslint-plugin-flowtype": "2.46.1",
        "eslint-plugin-import": "2.9.0",
        "eslint-plugin-promise": "3.6.0",
        "eslint-plugin-security": "1.4.0",
        "eslint-plugin-vue": "4.3.0",
        "markdownlint-cli": "0.7.1",
        "vue-eslint-parser": "2.0.3"
    },
    "peerDependencies": {
        "eslint": "^4.18.1",
        "babel-eslint": "^8.2.2",
        "eslint-plugin-ava": "^4.5.1",
        "eslint-plugin-flowtype": "^2.46.1",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-promise": "^3.6.0",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-vue": "^4.3.0",
        "vue-eslint-parser": "^2.0.3"
    }
}
